{
  "name": "downfall-landing",
  "version": "1.0.0",
  "description": "Downfall landing page, based on dimension by HTML5 UP",
  "main": "src/main.js",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "rm -rf dist/ && NODE_ENV=production parcel build src/index.html",
    "serve": "npm run build && serve dist/",
    "fmt": "eslint src/**/*.js --fix && prettier --write .",
    "cypress": "cypress open",
    "prepare": "husky install"
  },
  "author": "Christopher Bloom",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^5.6.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^6.0.0",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-imagemin": "^4.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.1",
    "serve": "^12.0.0"
  },
  "dependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "tailwindcss": "^1.9.6"
  }
}
